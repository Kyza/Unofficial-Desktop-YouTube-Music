{
  "name": "DesktopYouTubeMusic",
  "version": "1.0.0",
  "description": "Ever wanted to use YouTube Music but with the functionality of a desktop app like Spotify?",
  "main": "index.js",
  "dependencies": {
    "discord-rich-presence": "0.0.8",
    "discord-rpc": "^3.0.2",
    "electron-updater": "^4.1.2",
    "mousetrap": "^1.6.3",
    "opn": "^6.0.0",
    "request": "^2.88.0",
    "request-progress": "^3.0.0",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-builder": "^21.1.5"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build-linux": "electron-builder --linux",
    "build-all": "electron-builder -mwl"
  },
  "build": {
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyzaGitHub/Desktop-YouTube-Music.git"
  },
  "author": "Kyza",
  "email": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/KyzaGitHub/Desktop-YouTube-Music/issues"
  },
  "homepage": "https://github.com/KyzaGitHub/Desktop-YouTube-Music#readme"
}
